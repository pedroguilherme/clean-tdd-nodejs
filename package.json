{
  "name": "nodejs-com-typescript",
  "version": "1.2.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/main/server.js",
    "debug": "node --inspect=0.0.0.0:9222 --nolazy dist/main/server.js",
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "test": "jest --passWithNoTests --noStackTrace --no-watchman",
    "test:unit": "npm test -- --watch --runInBand --config=jest-unit.config.ts",
    "test:integration": "npm test -- --watch --config=jest-integration.config.ts",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage=true",
    "docker:up": "docker-compose up -d",
    "docker:stop": "docker-compose stop",
    "docker:test": "docker exec -t clean-tdd-node bash -c \"npm test --passWithNoTests --noStackTrace --no-watchman\"",
    "docker:rebuild": "docker-compose restart"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@shelf/jest-mongodb": "^2.2.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/node": "^17.0.14",
    "@types/supertest": "^2.0.11",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "git-commit-msg-linter": "^4.0.7",
    "husky": "^4.3.8",
    "jest": "^27.4.7",
    "lint-staged": "^12.3.3",
    "mockdate": "^3.0.5",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "fast-glob": "^3.2.11",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.3.1",
    "validator": "^13.7.0"
  },
  "engines": {
    "node": "16.x"
  }
}
